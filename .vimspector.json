{
	"configurations": {
		"py:launch": {
			"adapter": "debugpy",
			"filetypes": [
				"python"
			],
			"default": true,
			"configuration": {
				"request": "launch",
				"python": "./.venv/bin/python",
				"cwd": "${workspaceFolder}",
				"module": "${fileRelativePath//[\\/]/.}",
				"stopOnEntry": true,
				"debugOptions": [],
				"args": [
					"*${ProgramArgs}"
				]
			},
			"breakpoints": {
				"exception": {
					"raised": "N",
					"uncaught": "",
					"userUnhandled": ""
				}
			}
		}
	}
}